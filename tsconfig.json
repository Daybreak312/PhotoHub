{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "types": [
      "vite/client",
      "vite-plugin-svgr/client"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",

    /*"baseUrl": ".",
    "paths": {
      "@*//*": [
        "./src*//*"
      ],
      "@component*//*": [
        "./src/component*//*"
      ],
      "@asset*//*": [
        "./src/asset*//*"
      ],
      "@app*//*": [
        "./src/app*//*"
      ]
    }
  },*/
  "include": [
    "src"
  ]
}
